# Copy this to .env.local and add your API keys
ANTHROPIC_API_KEY=your-api-key-here

# Clerk Authentication
# Get these from https://dashboard.clerk.com
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
CLERK_SECRET_KEY=your-clerk-secret-key

# Redis for phrase storage
REDIS_URL=your-redis-url

# Axiom Logging (optional - for querying logs via /api/logs)
# Get these from https://axiom.co
AXIOM_TOKEN=your-axiom-api-token
AXIOM_DATASET=your-axiom-dataset-name
